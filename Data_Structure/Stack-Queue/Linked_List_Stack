
public class ArrayStack {
	private String[]s;
	private int N=0;
	
	public ArrayStack(int Capacity) {
		s=new String[Capacity];
	}
    public boolean isEmpty() {
    	return N==0;
    }
    public void push(String item) {
    	s[N++]=item;
    }
    public String pop() {
    	return s[--N];
    }
}
