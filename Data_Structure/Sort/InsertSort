
public class InsertionSort {
	public void sort(int[]nums) {
		for(int i=0;i<nums.length;i++) {
			for(int j=i;j>0;j--) {
				if(j-1>0) {
					if(nums[j-1]>nums[j]) {
						swap(nums,j,j-1);
					}
				}
			}
		
		}
 }
	public void swap(int[]nums,int i,int j) {
		int a=nums[i];
		nums[i]=nums[j];
		nums[j]=a;
	}
}
