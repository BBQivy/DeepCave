
public class SelectionSort {
	public void sort(int[]nums) {
	
		for(int i=0;i<nums.length;i++) {	
			nums[i]=small(nums,i);
		}
	}
	
	public int small(int[]nums,int j) {
	    int sm=0;   
	    for(int i=j;i<nums.length;i++) {
	    	if(i==j) {
	    		sm=nums[i];
	    	}
	    	else {
	    		if(nums[i]<sm) {
	    			sm=nums[i];
	    		}
	    	}
	    }
	return sm;} 
}
